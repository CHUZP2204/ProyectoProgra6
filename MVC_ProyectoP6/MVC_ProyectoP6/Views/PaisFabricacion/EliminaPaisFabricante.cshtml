
@{
    ViewBag.Title = "EliminaPaisFabricante";
}


<div class="jumbotron">
    <h2>EliminaPaisFabricante</h2>

    <br />
    <form id="frmEliminaPaisFabricacion" method="post">

        <div class="form-group">
            <label>Pais Fabricante</label>
            <input type="text" name="PaisFabricante" id="PaisFabricante" class="form-control" value="@Model.PaisFabricante" readonly />
        </div>

        <div class="form-group">
            <label> Codigo Pais Fabricante</label>
            <input type="text" name="CodigoPaisFabricante" id="CodigoPaisFabricante" class="form-control" value="@Model.CodigoPaisFabricante" readonly />
        </div>

        <div class="form-group">
            <input type="submit" value="Eliminar" id="btnEliminar" class="btn btn-danger" />
        </div>

    </form>
    <a href="~/PaisFabricacion/ListaPaisFabricante" class="btn btn-primary">Lista Paises</a>
</div>


