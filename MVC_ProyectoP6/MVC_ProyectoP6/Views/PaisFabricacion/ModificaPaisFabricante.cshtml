
@{
    ViewBag.Title = "ModificaPaisFabricante";
}

<h2>Modificar Pais Fabricante</h2>
<a href="~/PaisFabricacion/ListaPaisFabricante">Lista Paises Fabricantes</a>
<br />
<br />
<form id="frmModifcaPaisFabricante" method="post">

    <div class="form-group">
        <label>Pais Fabricante</label>
        <input type="text" name="PaisFabricante" id="PaisFabricante" class="form-control" value="@Model.PaisFabricante" />
    </div>

    <div class="form-group">
        <label> Codigo Pais Fabricante</label>
        <input type="text" name="CodigoPaisFabricante" id="CodigoPaisFabricante" class="form-control" value="@Model.CodigoPaisFabricante" />
    </div>

    <div class="form-group">
        <input type="submit" value="Modificar" id="btnModificar" />
    </div>

</form>



@section scripts{

    <script src="~/Scripts/PaisFabricacion/ModificaPaisFabricante.js"></script>
}

