
@{
    ViewBag.Title = "EliminaTipoVehiculo";
}

<h2>EliminaTipoVehiculo</h2>

<form id="frmEliminaTipoVehiculo" method="post">

    <div class="form-group">
        <label>Codigo Vehiculo </label>
        <input type="text" name="CodigoTipoVehiculo" id="CodigoTipoVehiculo" class="form-control" value="@Model.CodigoTipoVehiculo" readonly />
    </div>

    <div class="form-group">
        <label>Tipo Vehiculo</label>
        <input type="text" name="TipoVehiculo" id="TipoVehiculo" class="form-control" value="@Model.TipoVehiculo" readonly />
    </div>

    <div class="form-group">
        <input type="submit" value="Eliminar" id="btnEliminar" />
    </div>
    <br />
    <a href="~/TipoVehiculo/ListaTipoVehiculos">Lista Tipos de Vehiculo</a>



</form>

